<ProfessionFloatLayout>
    canvas.before:
        Rectangle:
            pos: self.pos
            size: self.size
            source: 'data/icons/profession/background.jpg'

    Button:
        size_hint: 0.12625,0.134
        pos_hint: {"x": 0.015, 'y': 0.84}
