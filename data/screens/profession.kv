        size_hint: 0.0575, 0.092
        pos_hint: {"x": 0.91375, 'y': 0.386}
        background_normal:  "data/icons/profession/right.jpg"
        background_down:    "data/icons/profession/right.jpg"
        on_release: root.go_next_screen()
